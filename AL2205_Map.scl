VAR_INPUT
    iDI_P2 : INT;
    iDI_P4 : INT;
    iDI_P6 : INT;
END_VAR

VAR_OUTPUT
    qDX_P2 : INT;
    qDX_P4 : INT;
    qDX_P6 : INT;
END_VAR

VAR
    DI_P2 :	ARRAY[0..15] OF BOOL;
    DI_P4 :	ARRAY[0..15] OF BOOL;
    DI_P6 :	ARRAY[0..15] OF BOOL;
    DX_P2 :	ARRAY[0..15] OF BOOL;
    DX_P4 :	ARRAY[0..15] OF BOOL;
    DX_P6 :	ARRAY[0..15] OF BOOL;
