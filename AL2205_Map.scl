"X1.1 PIN 2" :	INT;
"X1.2 PIN 2" :	INT;
"X1.3 PIN 2" :	INT;
"X1.4 PIN 2" :	INT;
"X1.5 PIN 2" :	INT;
"X1.6 PIN 2" :	INT;
"X1.7 PIN 2" :	INT;
"X1.0 PIN 4" :	INT;
"X1.1 PIN 4" :	INT;
